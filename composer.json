{
    "name": "karelwintersky/findfolks",
    "description": "Find Folks",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Karel Wintersky",
            "email": "karel.wintersky@yandex.ru"
        }
    ],
    "config": {
        "platform-check": false
    },
    "require": {

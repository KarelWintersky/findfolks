        "ext-curl": "*",
        "ext-gd": "*",
        "ext-mysqli": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-fileinfo": "*",
        "vlucas/phpdotenv": "^3.3",
        "foolz/sphinxql-query-builder": "^2.0",
        "karelwintersky/arris": "^1.74",
        "karelwintersky/arris.helpers": "^0.4.0",
        "psr/log": "^1.1",
        "spatie/regex": "^1.4",
        "karelwintersky/arris.toolkit.sphinx": "^1.9",
        "ajur-media/fluentpdo": "^1.0",
        "smarty/smarty": "^4.1",
        "nette/utils": "^3.2"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "FindFolks\\": "engine/"
        },
        "files": [
            "engine/helpers.php"
        ]
    }
}
